{
  "name": "phaser3-typescript",
  "version": "0.1.0",
  "description": "Boilerplate, Tutorials, Cheat Sheets and more for Phaser 3 in Typescript",
  "keywords": [
    "Phaser",
    "Phaser 3",
    "Phaser3",
    "TypeScript",
    "Webpack",
    "Tutorials",
    "Game Examples"
  ],
  "devDependencies": {
    "expose-loader": "0.7.5",
    "npm-run-all": "^4.1.5",
    "ts-loader": "5.3.2",
    "tsc-watch": "^4.2.9",
    "typescript": "3.2.2",
    "webpack": "4.28.3",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@types/node": "^14.14.35",
    "@types/socket.io": "^2.1.13",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "phaser": "^3.23.0",
    "socket.io": "^3.1.2",
    "socket.io-client": "^3.1.2"
  },
  "scripts": {
    "dev": "webpack --mode development && webpack-dev-server --mode development",
    "clientDev": "webpack --mode development && webpack-dev-server --mode development",
    "serverDev": "(tsc -p server/ --outDir server/build --esModuleInterop true) & (node server/build/socketServer.js)",
    "serverDevWatch": "tsc-watch ./server/socketServer.ts --outDir ./server/build --onSuccess 'node server/build/socketServer.js'",
    "serverDev2": "(tsc -p server2/ --outDir server2/build --esModuleInterop true) & (node server2/build/index.js)",
    "build": "webpack --mode production && webpack-dev-server --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dickinsonmr1/phaser3-platformer.git"
  },
  "author": {
    "name": "dickinsonmr1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dickinsonmr1/phaser3-platformer/issues"
  },
  "homepage": "https://github.com/dickinsonmr1/phaser3-platformer"
}
